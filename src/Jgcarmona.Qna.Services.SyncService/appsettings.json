{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithCorrelationId" ]
  },
  "CommonFeatureFlags": {
    "MessagingProvider": "RabbitMQ" // or "InMemoryEventDispatcher" or "AzureEventHub"
  },
  "RabbitMQSettings": {
    "HostName": "rabitmq",
    "UserName": "qnauser",
    "Password": "P@ssw0rd!",
    "QueueName": "events_queue",
    "ExchangeName": "events_exchange",
    "RoutingKey": "events.routingkey"
  }
}