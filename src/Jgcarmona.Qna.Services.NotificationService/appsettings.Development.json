{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341",
          "apiKey": "SeqApiKey"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithCorrelationId", "WithProperty" ],
    "Properties": {
      "ServiceName": "Jgcarmona.Qna.Services.NotificationService"
    }
  },
  "FeatureFlags": {
    "MigrateAtStartup": false,
    "MessagingProvider": "RabbitMQ",
    "SecretsProvider": "LocalEnv"
  }
}
